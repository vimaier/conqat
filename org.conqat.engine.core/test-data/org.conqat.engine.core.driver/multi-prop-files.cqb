<conqat xmlns="http://conqat.cs.tum.edu/ns/config"
	xmlns:cq="http://conqat.cs.tum.edu/ns/config">

<block-spec name="prop2">

	<param name="prop">
		<attr name="in"/>
		<attr name="in2"/>
  	</param>
  
    <processor name="data-sink" class="org.conqat.engine.core.driver.processors.DataSinkProcessor">
      <append name="prop.in" value="@prop.in"/>
      <append name="prop.in2" value="@prop.in2"/>
      <append name="prop2" value="value-in-config-file"/>
    </processor>

</block-spec>

</conqat>
