<conqat xmlns="http://conqat.cs.tum.edu/ns/config"
	xmlns:cq="http://conqat.cs.tum.edu/ns/config">

	<block-spec name="systestB">
		<param name="input">
			<attr name="value" />
		</param>
		<processor name="p"
			class="org.conqat.engine.core.driver.processors.StringGenerator">
			<set value="@input.value" />
		</processor>
		<out name="out" ref="@p" />
	</block-spec>

</conqat>
